algoritmo "Soma"
var
   N1, N2 : Inteiro
   M: Real
inicio
   Escreva ("Informe um número: ")
   Leia (N1)
   Escreva ("Informe outro número: ")
   Leia (N2)
   Escreval ("-----------------------")
   M <- (N1 + N2)/2
   Escreval ("A media entre ", N1, " e ", N2 , " e igual a ", M)
fimalgoritmo
