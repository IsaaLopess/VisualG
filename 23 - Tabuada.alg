algoritmo "Tabuada"
var
   contador, N: Inteiro
inicio
      contador <- 1
      Escreva ("Quer ver a tabuada de qual numero?: ")
      Leia (N)
        Repita
              Escreval (N, " x ", contador, "=", n * contador)
              contador <- contador + 1
        Ate (contador > 10)
fimalgoritmo
