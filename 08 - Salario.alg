algoritmo "Salario"
var
    Nome: Caractere
    Salario: Real
inicio
      Escreva ("Informe o nome do colaborador: ")
      Leia (Nome)
      Escreva ("Informe o salario dele: ")
      Leia (Salario)
      Escreval ("O colaborador ", Nome , " tem o salario de R$", Salario, ".")
fimalgoritmo
